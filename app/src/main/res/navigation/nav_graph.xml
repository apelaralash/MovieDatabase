            app:popExitAnim="@anim/fade_out"/>
        <action
            android:id="@+id/action_favorite_page_to_tvInfoFragment"
            app:destination="@id/tvInfoFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out"/>
    </fragment>
    <fragment
        android:id="@+id/search_page"
        android:name="com.example.myapplication.ui.fragments.SearchFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_search_page_to_searchResultFragment"
            app:destination="@id/searchResultFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out"/>
