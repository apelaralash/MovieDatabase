    </fragment>
    <fragment
        android:id="@+id/seasonInfoFragment"
        android:name="com.example.myapplication.ui.fragments.itemsinfos.SeasonInfoFragment"
        android:label="SeasonInfoFragment" >
        <argument
            android:name="mediaId"
            app:argType="long" />
        <argument
